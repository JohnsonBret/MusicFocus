<link rel="stylesheet" href="schedule.css">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">

<div class="flexColumnCenter width100Percent prompt">
    <h1 id="rehearsalScheduleTitle">Rehearsal Schedule</h1>
    <div id="scheduleControls" class="flexRow justifyCenter alignCenter width80Percent">
        <i id="prevWeekIconRehearsal" class="fas fa-chevron-left orangeIcon"></i>
        <h2 id="rehearsalScheduleSubTitle" class="scheduleSubTitle"></h2>
        <i id="nextWeekIconRehearsal" class="fas fa-chevron-right orangeIcon"></i>
    </div>
    <button id="refreshButtonRehearsal" class="button orangeBtn"><i class="fas fa-redo orangeIcon"></i></button>
</div>

<section id="rehearsalScheduleRoot" class="width100Percent" data-schedule-type="2">
    <div class="weekRehearsal prompt">
        <div class="dayRehearsal">
            <h2 class="textCenter">Mon</h2>
        </div>
        <div class="dayRehearsal">
            <h2 class="textCenter">Tue</h2>
        </div>
        <div class="dayRehearsal">
            <h2 class="textCenter">Wed</h2>
        </div>
        <div class="dayRehearsal">
            <h2 class="textCenter">Thu</h2>
        </div>
        <div class="dayRehearsal">
            <h2 class="textCenter">Fri</h2>
        </div>
        <div class="dayRehearsal">
            <h2 class="textCenter">Sat</h2>
        </div>
        <div class="dayRehearsal">
            <h2 class="textCenter">Sun</h2>
        </div>

    </div>

</section>