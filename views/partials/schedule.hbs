<link rel="stylesheet" href="schedule.css">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">

<div class="flexColumnCenter width100Percent">
    <h1>Schedule</h1>
    <div id="scheduleControls" class="flexRow justifyCenter alignCenter">
        <i id="prevWeekIcon" class="fas fa-chevron-left"></i>
        <h2 id="scheduleSubTitle"></h2>
        <i id="nextWeekIcon" class="fas fa-chevron-right"></i>
    </div>
    <button id="refreshButton" class="button"><i class="fas fa-redo"></i></button>
</div>

<section id="scheduleRoot">
    <div class="week flexRow blueBG width100Percent">
        <div class="day">
            <h2 class="textCenter">Mon</h2>
        </div>
        <div class="day">
            <h2 class="textCenter">Tue</h2>
        </div>
        <div class="day">
            <h2 class="textCenter">Wed</h2>
        </div>
        <div class="day">
            <h2 class="textCenter">Thu</h2>
        </div>
        <div class="day">
            <h2 class="textCenter">Fri</h2>
        </div>
        <div class="day">
            <h2 class="textCenter">Sat</h2>
        </div>
        <div class="day">
            <h2 class="textCenter">Sun</h2>
        </div>

    </div>

</section>
<script src="/luxon.min.js"></script>
<script src="/schedule.js"></script>